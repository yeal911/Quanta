{
  "Hotkey": {
    "Modifier": "Alt",
    "Key": "Space"
  },
  "SearchDirectories": [
    "C:\\Users\\{username}\\Desktop",
    "C:\\Users\\{username}\\Downloads"
  ],
  "MaxRecentFiles": 30,
  "AutoStart": false,
  "Commands": [
    {
      "Keyword": "b",
      "Name": "百度搜索",
      "Type": "Url",
      "Template": "https://www.baidu.com/s?wd={param}"
    },
    {
      "Keyword": "g",
      "Name": "Google搜索",
      "Type": "Url",
      "Template": "https://www.google.com/search?q={param}"
    },
    {
      "Keyword": "gh",
      "Name": "GitHub",
      "Type": "Url",
      "Template": "https://github.com/search?q={param}"
    },
    {
      "Keyword": ">",
      "Name": "PowerShell",
      "Type": "Shell",
      "Template": "{param}"
    },
    {
      "Keyword": "calc",
      "Name": "计算器",
      "Type": "Calculator",
      "Template": "{param}"
    }
  ]
}
